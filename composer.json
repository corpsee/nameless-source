{
    "name":        "corpsee/nameless-source",
    "description": "Yet another framework based on Symfony Components.",
    "type":        "framework",
    "keywords": [
        "framework",
        "nameless"
    ],
    "license": "GPL-3.0",
    "authors": [
        {
            "name":     "Corpsee",
            "email":    "poisoncorpsee@gmail.com",
            "homepage": "http://corpsee.com",
            "role":     "Developer"
        }
    ],
    "require": {
        "php":                       ">=5.4.0",
        "ext-mbstring":              "*",

        "psr/log":                   "1.0.*",
        "ircmaxell/password-compat": "1.0.*",
        "pimple/pimple":             "3.0.*",
        "symfony/http-foundation":   "2.5.*",
        "symfony/http-kernel":       "2.5.*",
        "symfony/routing":           "2.5.*",
        "symfony/event-dispatcher":  "2.5.*",
        "symfony/debug":             "2.5.*",
        "symfony/console":           "2.5.*",
        "swiftmailer/swiftmailer":   "5.3.*",
        "monolog/monolog":           "1.12.*",
        "kriswallsmith/assetic":     "1.2.*",
        "oyejorge/less.php":         "1.7.*",
        "natxet/CssMin":             "3.0.*",
        "meenie/javascript-packer":  "1.*",
        "robmorgan/phinx":           "0.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "psr-4": {
            "Nameless\\":        "src",
            "Nameless\\Tests\\": "tests"
        },
        "files": [
            "src/Core/functions/functions.php"
        ]
    },
    "minimum-stability": "dev"
}
