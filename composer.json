{
	"name":        "corpsee/nameless-source",
	"description": "Yet another framework based on Symfony Components.",
	"type":        "library",
	"keywords":
	[
		"framework",
		"nameless"
	],
	"license":     "MIT",
	"authors":
	[
		{
			"name":     "Corpsee",
			"email":    "poisoncorpsee@gmail.com",
			"homepage": "http://corpsee.com",
			"role":     "Developer"
		}
	],
	"require":
	{
		"pimple/pimple":            "v1.1.*",
		"symfony/http-foundation":  "v2.3.*",
		"symfony/http-kernel":      "v2.3.*",
		"symfony/routing":          "v2.3.*",
		"symfony/event-dispatcher": "v2.3.*",
		"symfony/debug":            "v2.3.*",
		"symfony/console":          "v2.3.*",
		"swiftmailer/swiftmailer":  "v5.0.*",
		"monolog/monolog":          "1.7.*",
		"kriswallsmith/assetic":    "v1.1.*",
		"leafo/lessphp":            "v0.4.*"
	},
	"require-dev":
	{
		"phpunit/phpunit":          "3.7.*"
	},
	"autoload":
	{
		"psr-0":
		{
			"Nameless":    "src/"
		},
		"files":
		[
			"src/Nameless/Core/functions/functions.php"
		]
	},
	"minimum-stability": "dev"
}
