{
    "name":        "corpsee/nameless-source",
    "description": "Yet another framework based on Symfony Components.",
    "type":        "framework",
    "keywords": [
        "framework",
        "nameless"
    ],
    "license": "GPL-3.0",
    "authors": [
        {
            "name":     "Corpsee",
            "email":    "poisoncorpsee@gmail.com",
            "homepage": "http://corpsee.com",
            "role":     "Developer"
        }
    ],
    "require": {
        "php":                       ">=5.4.0",
        "ext-mbstring":              "*",

        "psr/log":                   "~1.0.0",
        "ircmaxell/password-compat": "~1.0.0",
        "pimple/pimple":             "~3.0.0",
        "symfony/http-foundation":   "~2.6.0",
        "symfony/http-kernel":       "~2.6.0",
        "symfony/routing":           "~2.6.0",
        "symfony/event-dispatcher":  "~2.6.0",
        "symfony/debug":             "~2.6.0",
        "symfony/console":           "~2.6.0",
        "swiftmailer/swiftmailer":   "~5.4.0",
        "monolog/monolog":           "~1.12.0",
        "kriswallsmith/assetic":     "~1.2.0",
        "oyejorge/less.php":         "~1.7.0",
        "natxet/CssMin":             "~3.0.0",
        "meenie/javascript-packer":  "~1.0",
        "robmorgan/phinx":           "~0.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "psr-4": {
            "Nameless\\":        "src",
            "Nameless\\Tests\\": "tests"
        },
        "files": [
            "src/Core/functions/functions.php"
        ]
    },
    "minimum-stability": "dev"
}
