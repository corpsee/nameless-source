{
    "name":        "corpsee/nameless-source",
    "description": "Yet another framework based on Symfony Components.",
    "type":        "framework",
    "keywords": [
        "framework",
        "nameless"
    ],
    "license": "MIT",
    "authors": [
        {
            "name":     "Corpsee",
            "email":    "poisoncorpsee@gmail.com",
            "homepage": "http://corpsee.com",
            "role":     "Developer"
        }
    ],
    "require": {
        "php":                      ">=5.4.0",
        "psr/log":                  "1.0.*",
        "pimple/pimple":            "1.1.*",
        "symfony/http-foundation":  "2.3.*",
        "symfony/http-kernel":      "2.3.*",
        "symfony/routing":          "2.3.*",
        "symfony/event-dispatcher": "2.3.*",
        "symfony/debug":            "2.3.*",
        "symfony/console":          "2.3.*",
        "swiftmailer/swiftmailer":  "5.2.*",
        "monolog/monolog":          "1.7.*",
        "kriswallsmith/assetic":    "1.1.*",
        "oyejorge/less.php":        "1.7.*",
        "natxet/CssMin":            "3.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "psr-4": {
            "Nameless\\":        "src",
            "Nameless\\Tests\\": "tests"
        },
        "files": [
            "src/Core/functions/functions.php"
        ]
    },
    "minimum-stability": "dev"
}
